{
  "applications": {
    "gecko": {
      "id": "@colortab",
      "strict_min_version": "54.0"
    }
  },
  "manifest_version": 2,
  "author": "wildskyf",
  "name": "Color Tab",
  "version": "2.0.0",

  "description": "This addon would replace your boring new-tab page to a page providing some random colors.",

  "homepage_url": "https://github.com/wildskyf/color-tab",

  "chrome_url_overrides": {
    "newtab": "data/index.html"
  },

  "license": "MIT",

  "keywords": [
    "Random", "Color", "Palettab"
  ],

  "web_accessible_resources": [
    "data/colors.json"
  ]
}
